{
  "name": "Airframe",
  "version": "2.0.0",
  "description": "A project starter kit",
  "repository": {
    "type": "git",
    "url": "https://github.com/planetary/airframe.git"
  },
  "author": "Planetary",
  "bugs": {
    "url": "https://github.com/planetary/airframe/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/planetary/airframe",
  "engines": {
    "node": ">=4.2.0",
    "npm": ">=2.1.18"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=prod webpack -p",
    "postbuild": "imagemin assets/images/* --out-dir=build/images",
    "prebuild:dev": "rimraf build",
    "build:dev": "cross-env NODE_ENV=dev webpack",
    "postbuild:dev": "imagemin assets/images/* --out-dir=build/images",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --inline --hot",
    "validate": "npm-run-all --parallel lint build",
    "lint": "eslint assets"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "copy-webpack-plugin": "^3.0.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "css-modularscale": "^1.1.1",
    "eslint": "^3.2.2",
    "eslint-config-planetary": "^0.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "imagemin-cli": "^3.0.0",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "npm-run-all": "^2.3.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^7.1.3",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^5.0.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "pug": "^2.0.0-beta4",
    "pug-loader": "^2.2.1",
    "pug-walk": "0.0.3",
    "reload-html-webpack-plugin": "^0.1.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-webpack-plugin": "^0.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-config-utils": "^2.0.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-validator": "^2.2.7"
  }
}
