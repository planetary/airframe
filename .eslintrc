{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": false,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": false,
        "generators": true,
        "modules": false,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": false,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "octalLiterals": false,
        "regexUFlag": false,
        "regexYFlag": false,
        "restParams": true,
        "spread": true,
        "superInFunctions": false,
        "templateStrings": true,
        "unicodeCodePointEscapes": false,
        "globalReturn": false,
        "jsx": true
    },

    "parser": "babel-eslint",

    "env": {
        "browser": false,
        "es6": true,
        "node": true
    },

    "rules": {
        "no-array-constructor": 1,
        "no-arrow-condition": 1,
        "no-bitwise": 1,
        "no-caller": 1,
        "no-case-declarations": 0,
        "no-catch-shadow": 1,
        "no-class-assign": 1,
        "no-cond-assign": 1,
        "no-const-assign": 1,
        "no-continue": 0,
        "no-console": 0,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-delete-var": 1,
        "no-div-regex": 0,
        "no-dupe-args": 1,
        "no-dupe-class-members": 1,
        "no-dupe-keys": 1,
        "no-duplicate-case": 1,
        "no-else-return": 1,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-empty-label": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-ex-assign": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 0,
        "no-extra-semi": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 1,
        "no-func-assign": 1,
        "no-implicit-coercion": 1,
        "no-implied-eval": 1,
        "no-inline-comments": 0,
        "no-inner-declarations": [1, "functions"],
        "no-invalid-regexp": 1,
        "no-invalid-this": 0,
        "no-irregular-whitespace": 1,
        "no-iterator": 1,
        "no-label-var": 1,
        "no-labels": 1,
        "no-lone-blocks": 1,
        "no-lonely-if": 1,
        "no-loop-func": 0,
        "no-magic-numbers": 0,
        "no-mixed-requires": [1, true],
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-multiple-empty-lines": [1, {"max": 2, "maxEOF": 1}],
        "no-native-reassign": 1,
        "no-negated-condition": 1,
        "no-negated-in-lhs": 1,
        "no-nested-ternary": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-object": 1,
        "no-new-require": 1,
        "no-new-wrappers": 1,
        "no-obj-calls": 1,
        "no-octal": 1,
        "no-octal-escape": 1,
        "no-param-reassign": 0,
        "no-path-concat": 1,
        "no-plusplus": 0,
        "no-process-env": 0,
        "no-process-exit": 1,
        "no-proto": 1,
        "no-redeclare": 1,
        "no-regex-spaces": 1,
        "no-restricted-modules": 0,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-shadow": [1, {"builtinGlobals": true, "allow": ["err", "next"]}],
        "no-shadow-restricted-names": 1,
        "no-spaced-func": 1,
        "no-sparse-arrays": 1,
        "no-sync": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-this-before-super": 1,
        "no-throw-literal": 1,
        "no-undef": 1,
        "no-undef-init": 1,
        "no-undefined": 1,
        "no-underscore-dangle": 0,
        "no-unexpected-multiline": 1,
        "no-unneeded-ternary": [1, {"defaultAssignment": false}],
        "no-unreachable": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
        "no-use-before-define": 1,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-void": 0,
        "no-var": 1,
        "no-warning-comments": [1, {"terms": ["todo", "fixme"], "location": "start"}],
        "no-with": 1,

        "accessor-pairs": 1,
        "array-bracket-spacing": [1, "never"],
        "arrow-body-style": [1, "as-needed"],
        "arrow-parens": [1, "always"],
        "arrow-spacing": 1,
        "block-scoped-var": 1,
        "block-spacing": [1, "always"],
        "brace-style": [1, "1tbs"],
        "callback-return": [1, ["next"]],
        "camelcase": 1,
        "comma-dangle": [1, "never"],
        "comma-spacing": [1, {"before": false, "after": true}],
        "comma-style": [1, "last"],
        "computed-property-spacing": [1, "never"],
        "consistent-return": 0,
        "consistent-this": [1, "that"],
        "constructor-super": 1,
        "curly": [1, "multi-or-nest", "consistent"],
        "default-case": 0,
        "dot-location": [1, "property"],
        "dot-notation": [1, {"allowKeywords": true, "allowPattern": "_"}],
        "eol-last": 1,
        "eqeqeq": 1,
        "func-names": 0,
        "func-style": [1, "expression"],
        "generator-star-spacing": [1, "before"],
        "global-require": 0,
        "guard-for-in": 0,
        "handle-callback-err": 1,
        "id-length": [1, {"min": 1, "max": 20, "properties": "never"}],
        "indent": [1, 4, {"SwitchCase": 1}],
        "init-declarations": 0,
        "jsx-quotes": [1, "prefer-double"],
        "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
        "linebreak-style": [1, "unix"],
        "lines-around-comment": [1, {
            "beforeBlockComment": true,
            "afterBlockComment": false,
            "beforeLineComment": true,
            "afterLineComment": false,
            "allowBlockStart": true,
            "allowObjectStart": true,
            "allowArrayStart": true
        }],
        "new-parens": 1,
        "max-depth": 0,
        "max-len": [1, 99, 4, {"ignoreUrls": true}],
        "max-nested-callbacks": 0,
        "max-params": [1, 5],
        "max-statements": 0,
        "new-cap": 0,
        "new-parens": 1,
        "object-curly-spacing": [1, "never"],
        "object-shorthand": [1, "always"],
        "one-var": [1, "never"],
        "operator-assignment": [1, "always"],
        "operator-linebreak": 1,
        "padded-blocks": [1, "never"],
        "prefer-arrow-callback": 0,
        "prefer-const": 1,
        "prefer-reflect": 0,
        "prefer-spread": 1,
        "prefer-template": 1,
        "quotes": [1, "single", "avoid-escape"],
        "quote-props": [1, "as-needed", {"keywords": true, "numbers": true}],
        "require-jsdoc": 0,
        "require-yield": 1,
        "semi": 1,
        "semi-spacing": [1, {"before": false, "after": true}],
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "never"],
        "space-before-keywords": [1, "always"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-return-throw-case": 1,
        "space-unary-ops": [1, {"words": true, "nonwords": false}],
        "spaced-comment": [1, "always"],
        "strict": 0,
        "use-isnan": 1,
        "valid-jsdoc": 0,
        "valid-typeof": 1,
        "vars-on-top": 0,
        "wrap-iife": [1, "inside"],
        "wrap-regex": 0,
        "yoda": [1, "never", {"exceptRange": true}]
    }
}
